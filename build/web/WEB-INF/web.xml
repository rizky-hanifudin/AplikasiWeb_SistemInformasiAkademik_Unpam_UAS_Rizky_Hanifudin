    <servlet>
        <servlet-name>MataKuliahController</servlet-name>
        <servlet-class>com.unpam.controller.MataKuliahController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BiayaKuliahController</servlet-name>
        <servlet-class>com.unpam.controller.BiayaKuliahController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PembayaranController</servlet-name>
        <servlet-class>com.unpam.controller.PembayaranController</servlet-class>
