    <servlet>
        <servlet-name>LaporanNilaiController</servlet-name>
        <servlet-class>com.unpam.controller.LaporanNilaiController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LaporanPembayaranController</servlet-name>
        <servlet-class>com.unpam.controller.LaporanPembayaranController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LaporanKaryawanController</servlet-name>
        <servlet-class>com.unpam.controller.LaporanKaryawanController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainForm</servlet-name>
        <url-pattern>/MainForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MahasiswaController</servlet-name>
        <url-pattern>/MahasiswaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KaryawanController</servlet-name>
        <url-pattern>/KaryawanController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MataKuliahController</servlet-name>
        <url-pattern>/MataKuliahController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BiayaKuliahController</servlet-name>
        <url-pattern>/BiayaKuliahController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PembayaranController</servlet-name>
        <url-pattern>/PembayaranController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
